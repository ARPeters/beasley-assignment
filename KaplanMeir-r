library(survival)


cdata<-cancer
summary(survfit(surv(days, status==1)~1, data=cdata))
plot(survfit(surv(days, status==1)~1, data=cdata))

survdiff(Surv(days, status==1)~sex, data=cdata))
plot(survdiff(Surv(days, status==1)~sex, data=cdata)), conf.int=TRUE)

